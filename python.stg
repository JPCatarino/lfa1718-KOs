//templates

unitClass(cname,stat) ::= <<
from unitClass import unitDec
class <cname>:
    UnitDic = {}
    <stats(stat)>
>>

module(stat) ::= <<
def main():
    <stats(stat)>
>>

stats(stat) ::= <<
<if(stat)><stat; separator="\n"><endif>
>>

div(left,right) ::= <<
<left>/<right>
>>

variable(type,name) ::= " "

assign(left,right) ::= "<left> = <right>"

unit(uname,upot)::= <<
unitDec("<uname>",<upot>)
>>

createunit(uname,unit) ::=<<
UnitDic['<uname>'] = <unit>
>>


